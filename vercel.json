{
  "redirects": [
    { "source": "/(.*)", "destination": "/index.html", "statusCode": 200 }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
